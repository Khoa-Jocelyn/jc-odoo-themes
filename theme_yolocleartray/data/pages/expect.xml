<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record id="yolo_expect_page" model="website.page">
			<field name="name">Yolo Clear Tray - Chuyên gia</field>
			<field name="website_published">True</field>
			<field name="url">/expect</field>
			<field name="type">qweb</field>
			<field name="key">theme_yolocleartray.yolo_expect_page</field>
			<field name="arch" type="xml">
				<t t-name="theme_yolocleartray.yolo_expect_page">
